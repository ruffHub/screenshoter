{
  "name": "comodo-screenshoter",
  "version": "0.0.1",
  "description": "Utilites used in COMODO ITSM project",
  "main": "dist/Utils.js",
  "dependencies": {
    "html2canvas": "^1.0.0-alpha.12",
    "konva": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "eslint": "^5.5.0",
    "itsm-frontend-versions-manager": "git+ssh://git@git.mdm.comodo.od.ua:frontend/itsm-frontend-versions-manager.git#0.3.0",
